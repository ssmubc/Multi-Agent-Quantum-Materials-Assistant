# Amazon Braket MCP Server Configuration
# Copy this file to .env and fill in your values

# AWS Configuration (required for Braket)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Optional: Use AWS Profile instead of access keys
# AWS_PROFILE=your_profile_name

# Braket-specific Configuration
BRAKET_DEFAULT_DEVICE_ARN=arn:aws:braket:::device/quantum-simulator/amazon/sv1
BRAKET_WORKSPACE_DIR=./quantum_workspace

# Optional: S3 Configuration for storing results
BRAKET_S3_BUCKET=your-quantum-results-bucket
BRAKET_S3_PREFIX=experiments/

# Materials Project API Key (if not using AWS Secrets Manager)
MATERIALS_PROJECT_API_KEY=your_mp_api_key_here